<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controleur.Secretaire.Patient.ConsulterPatientController">
    <Label layoutX="154.0" layoutY="47.0" prefHeight="50.0" prefWidth="311.0" text="Consulter Patient" textFill="#970d50">
        <font>
            <Font name="Berlin Sans FB" size="36.0" />
        </font>
    </Label>
    <TableView fx:id="tablePatient" layoutY="162.0" prefHeight="160.0" prefWidth="600.0">
        <columns>
              
            <TableColumn fx:id="tv_ListExamen_cl_ID" minWidth="50.0" maxWidth="50.0" text="ID" />
            <TableColumn fx:id="tv_ListExamen_cl_Nom" minWidth="110.0" maxWidth="110.0" text="Nom" />
            <TableColumn fx:id="tv_ListExamen_cl_Prenom" minWidth="110.0" maxWidth="110.0" text="Prenom" />
            <TableColumn fx:id="tv_ListExamen_cl_GenrePatient" minWidth="110.0" maxWidth="110.0" text="SEXE" />
            <TableColumn fx:id="tv_ListExamen_cl_Tel" minWidth="110.0" maxWidth="110.0" text="Telephone" />
            <TableColumn fx:id="tv_ListExamen_cl_Date" minWidth="110.0" maxWidth="110.0" text="Date" />
        </columns>
    </TableView>
        
        
    <ButtonBar layoutX="356.0" layoutY="350.0" prefHeight="56.0" prefWidth="200.0">
        <buttons>
            <JFXButton  onAction="#SupprimerPatient" style="-fx-background-color: lightgreen;" text="Supprimer Patient">
                <font>
                    <Font name="System Bold" size="15.0" />
                </font>
            </JFXButton>
            <JFXButton onAction="#btnPatient" cancelButton="true" contentDisplay="CENTER" prefHeight="40.0" style="-fx-background-color: lightgreen;" text="Annuler" textAlignment="CENTER">
                <font>
                    <Font name="System Bold" size="15.0" />
                </font>
            </JFXButton>
        </buttons>
    </ButtonBar>
</AnchorPane>
